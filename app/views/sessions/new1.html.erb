<!-- app/views/sessions/new.html.erb -->
<h1>Register</h1>

<%= form_with model: @user, url:'/login' do |form| %>
  <%= form.label :email %>
  <%= form.text_field :email %>

  <%= form.label :password %>
  <%= form.text_field :password %>

  
  <%= form.submit "Register" %>
<% end %>
<%= link_to "login" , "login_user"%>


